{
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "plugins": [
      {
        "name": "@nestjs/swagger",
        "options": {
          "dtoFileNameSuffix": [".dto.ts", ".schema.ts", ".params.ts", ".query.ts", ".payload.ts", "exception.ts"]
        }
      }
    ]
  }
}
